  "name": "car_service_base",
  "version": "1.0.0",
  "description": "Application that allow car services to add informations about cars",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --config webpack.local.js",
    "start:dev": "webpack-dev-server --mode development --open --config webpack.dev.js",
    "build": "webpack --progress --bail --config webpack.local.js",
    "build:dev": "webpack --progress --bail --config webpack.dev.js",
    "build:prod": "webpack --progress --bail --config webpack.prod.js",
    "check-types": "tsc",
    "pretty": "prettier --write src/**/*.tsx src/**/*.ts src/**/*.scss",
    "lint": "eslint --ext .jsx --ext .js --ext .ts --ext .tsx src",
    "lint:fix": "eslint --ext .jsx --ext .js --ext .ts --ext .tsx src --fix",
    "precommit": "npm run check-types && npm run pretty && npm run lint:fix && git add ."
  },
